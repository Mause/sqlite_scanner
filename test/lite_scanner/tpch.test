statement ok
LOAD '../build/debug/extension/sqlite_scanner/sqlite_scanner.duckdb_extension';

statement ok
CALL sqlite_attach('tpch.db');

require tpch

loop i 1 9

query I
PRAGMA tpch(${i})
----
<FILE>:duckdb/extension/tpch/dbgen/answers/sf0.1/q0${i}.csv

endloop


loop i 10 15

query I
PRAGMA tpch(${i})
----
<FILE>:duckdb/extension/tpch/dbgen/answers/sf0.1/q${i}.csv

endloop

loop i 16 23

query I
PRAGMA tpch(${i})
----
<FILE>:duckdb/extension/tpch/dbgen/answers/sf0.1/q${i}.csv

endloop

# run Q15 single-threaded: it is not deterministic when run on doubles with multi-threading enabled
statement ok
SET threads=1

query I
PRAGMA tpch(15)
----
<FILE>:duckdb/extension/tpch/dbgen/answers/sf0.1/q15.csv
