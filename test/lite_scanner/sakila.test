query II
select A.store_id, B.sales 
from store A
join (
	select cus.store_id, floor(sum(pay.amount)) sales
	from customer cus
	join payment pay
	on pay.customer_id = cus.customer_id
group by cus.store_id
) B
on A.store_id = B.store_id
order by a.store_id;
